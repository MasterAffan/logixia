{
  "name": "logixia",
  "version": "1.0.3",
  "description": "Logixia - A next-generation logging library with advanced features, custom levels, and TypeScript IntelliSense support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "dev:basic-usage": "ts-node examples/basic-usage.ts",
    "dev:express": "ts-node examples/express-example.ts",
    "dev:nestjs": "ts-node examples/nestjs-example.ts",
    "dev:custom-levels": "ts-node examples/custom-levels.ts",
    "dev:performance": "ts-node examples/performance-monitoring.ts",
    "dev:fields": "ts-node examples/field-configuration.ts",
    "dev:search": "ts-node examples/search-quickstart.ts",
    "dev:intelligent-search": "ts-node examples/intelligent-search.ts"
  },
  "keywords": [
    "logging",
    "logger",
    "typescript",
    "nestjs",
    "express",
    "structured-logging",
    "trace-id",
    "performance",
    "middleware",
    "transport",
    "database-logging",
    "file-logging",
    "console-logging",
    "analytics",
    "monitoring",
    "enterprise",
    "production-ready",
    "open-source",
    "mit-license",
    "async-logging",
    "batch-processing",
    "log-rotation",
    "custom-levels",
    "child-logger",
    "error-tracking",
    "log-search",
    "intelligent-search",
    "nlp",
    "natural-language",
    "log-aggregation",
    "pattern-recognition",
    "anomaly-detection",
    "log-correlation",
    "ai-powered",
    "search-engine"
  ],
  "author": {
    "name": "Logixia Contributors",
    "email": "logixia@example.com",
    "url": "https://github.com/Logixia/logixia"
  },
  "contributors": [
    {
      "name": "Community Contributors",
      "url": "https://github.com/Logixia/logixia/graphs/contributors"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Logixia/logixia.git"
  },
  "bugs": {
    "url": "https://github.com/Logixia/logixia/issues"
  },
  "homepage": "https://github.com/Logixia/logixia#readme",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "express": "^4.18.0",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "express": "^4.18.0",
    "jest": "^29.0.0",
    "mongodb": "^6.19.0",
    "mysql2": "^3.14.5",
    "pg": "^8.16.3",
    "prettier": "^3.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
      "tsdown": "^0.15.0",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
